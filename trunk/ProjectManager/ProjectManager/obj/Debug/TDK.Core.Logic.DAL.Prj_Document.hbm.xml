<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="TDK.Core.Logic.DAL.Prj_Document,TDK.Core.Logic" table="Prj_Document" lazy="true">

		<id name="ID" column="ID" type="Int32" unsaved-value="-1">
			<generator class="increment"/>
		</id>
		<property column="DocumentName" type="String" name="DocumentName" length="100" />
		<property column="DocumentCaption" type="String" name="DocumentCaption" length="50" />
		<property column="CreateTime" type="DateTime" name="CreateTime" />
		<property column="ModifyTime" type="DateTime" name="ModifyTime" />
		<property column="Sequence" type="Int32" name="Sequence" />
		<property column="Type" type="String" name="Type" length="50" />
		<property column="TranslatorResult" type="String" name="TranslatorResult" length="50" />
		<property column="changed" type="String" name="changed" length="50" />
		<property column="Prj_Controller_ID" type="Int32" name="Prj_Controller_ID" not-null="true" />
		
	</class>
</hibernate-mapping>
